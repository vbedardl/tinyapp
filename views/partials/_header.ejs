<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/">Why TinyUrl?</a>
        <a class="nav-item nav-link" href="/">Features</a>
        <a class="nav-item nav-link" href="/">Pricing</a>
        <% if(user) { %>
          <a class="nav-item nav-link" href="/urls"> URLs</a>
          <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
          <% if(user.paid) {%>
          <a class="nav-item nav-link" href="/analytics">Analytics</a>
          <%}%>
        <% } %>
      </div>
    </div>
      <% if(user) {%>
        <form method="POST" action="/logout">
          <p class="navbar-brand"><%= user.email %></p>
          <button type="submit" class="btn btn-primary">Logout</button>
        </form>
      <% }else {%>
        <a class="text-white mr-3" href="/register">Register</a>
        <a class="text-white mr-3" href="/login">Login</a>
      <% } %>
  </nav>
</header>